<h1>To do list.</h1>

<% @tasks.each do |task| %>
  <h3 ><%= link_to task.title, task_path(task) %></h3>
  <p><%= task.details %></p>
  <% if task.completed == true %>
    <img src="data:image/png;base64,">
  <% else %>
    <img src="data:image/png;base64,">
  <% end %>
<% end %>
<br>

<%= link_to "New task", new_task_path %>



